<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<GridPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="catalogo.Controller">
    <BorderPane minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
        <center>
            <TableView prefHeight="369.0" prefWidth="628.0" BorderPane.alignment="CENTER" fx:id="tbFornecedores">
                <columns>
                    <TableColumn fx:id="colCodigo" prefWidth="55.0" text="Código" />
                    <TableColumn fx:id="colNome" prefWidth="250.0" text="Nome" />
                    <TableColumn fx:id="colCNPJ" minWidth="0.0" prefWidth="210.0" text="CNPJ" />
                    <TableColumn fx:id="colEndereco" minWidth="0.0" prefWidth="190.0" text="Endereço" />
                    <TableColumn fx:id="colTelefone" prefWidth="94.0" text="Telefone" />
                </columns>
            </TableView>
        </center>
        <bottom>
            <VBox alignment="TOP_CENTER" prefHeight="39.0" prefWidth="645.0">
                <children>
                    <Button mnemonicParsing="false" onAction="#abrirFornecedores" text="Gerenciar Fornecedores" />
                </children>
                <padding>
                    <Insets bottom="10.0" top="5.0" />
                </padding>
            </VBox>
        </bottom>
    </BorderPane>
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
</GridPane>
